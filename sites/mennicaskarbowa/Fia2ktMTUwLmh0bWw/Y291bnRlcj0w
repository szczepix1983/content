	<script>
		document.querySelector('.opinions_badge__wrapper').addEventListener('click', function openOpinionsBadge() {
			this.parentNode.classList.add('--open');
		});
		document.querySelector('.opinions_badge__close').addEventListener('click', function closeOpinionsBadge() {
			this.parentNode.parentNode.classList.remove('--open');
		});
		window.addEventListener('scroll', function scrollOpinionsBadge() {
			if (window.scrollY > 50) {
				document.querySelector('.opinions_badge').classList.add('--hide');
			} else {
				document.querySelector('.opinions_badge').classList.remove('--hide');
			}
		});
	</script><script>
