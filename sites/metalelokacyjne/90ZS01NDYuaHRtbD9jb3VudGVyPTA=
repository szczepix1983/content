        $('#longdescription_more').hide();

    });

    $('#component_deposit > div').html(depozyt);
    $('#component_payment_methods > div').html(platnosci);
    $('#component_lowest_prices > div').html(ceny);
    $('#component_dostawa24h > div').html(dostawa);
};</script>
