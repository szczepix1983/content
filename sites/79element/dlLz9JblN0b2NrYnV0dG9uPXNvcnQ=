    // menu highlight

    const items = document.getElementById("menu").children[0].children

    for (let i = 0; i < items.length; i++) {
        if(items[i].classList.contains("highlight")){
            items[i].addEventListener("click",() => {
                sessionStorage.removeItem("current")
                sessionStorage.setItem("current",items[i].id)
            })
        } else {
            items[i].addEventListener("click",() => {
                sessionStorage.removeItem("current")
                sessionStorage.setItem("current","")
            })
        }
    }

