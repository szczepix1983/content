                <script>
                function prepareRecaptcha()
                {
                    var captchableElems = [];
                var elementsToAdd = $('[name="mailing_email"]');
                if (elementsToAdd) {
                    for (var i = 0; i < elementsToAdd.length; i++) {
                        captchableElems.push(elementsToAdd[i]);
                    }
                }
                var elementsToAdd = $('[name="client_login"]');
                if (elementsToAdd) {
                    for (var i = 0; i < elementsToAdd.length; i++) {
                        captchableElems.push(elementsToAdd[i]);
                    }
                }
                    if (captchableElems.length > 0) {
                        var captchaScript = document.createElement('script');
                        captchaScript.onload = function() {
                            grecaptcha.ready(function() {
                                $("body").append('<div id="googleRecaptchaBadge" style="position: relative; left: -10px;">');
                                var clientId = grecaptcha.render('googleRecaptchaBadge', {
                                    'sitekey': '6LfY2KIUAAAAAHkCraLngqQvNxpJ31dsVuFsapft',
                                    'badge': 'bottomleft',
                                    'size': 'invisible'
                                });
                                grecaptcha.execute(clientId, {action: 'registration'}).then(function (token) {
                                    for (var i = 0; i < captchableElems.length; i++) {
                                        $(captchableElems[i]).closest("form").append('<input name="iai-recaptcha-token" value="' + token + '" type="hidden">');
                                    }
                                });
                            });
                        }
                        captchaScript.src = "https://www.google.com/recaptcha/api.js?render=explicit";
                        document.head.appendChild(captchaScript);
                   }
                }
                var applied = false;
                $('input[name=mailing_email],input[name=client_login], input[name=client_password], input[name=client_firstname], input[name=client_lastname], input[name=client_email], input[name=terms_agree]').focus(function() {
                    if (!applied) {
                        prepareRecaptcha();
                        applied = true;
                    }
                });
                </script><img src="https://client4744.idosell.com/checkup.php?c=75b0865932016a3b6592ed50215afc3b" style="display:none" alt="pixel"></body></html>
