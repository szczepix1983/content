                            <!-- Start Custom JS -->
                <script >$(document).ready(function () {
    $(".show_hide_content").hide();
    $(".show_hide").on("click", function () {
        var txt = $(".show_hide_content").is(':visible') ? 'czytaj więcej' : 'zwiń';
        $(".show_hide").text(txt);
        $(this).next('.show_hide_content').slideToggle(200);
    });
});

function ReloadFeaturesOnProductBaseOnURL() {
    if (window.location.href.split('#').length == 2) {
        var params = window.location.href.split('#')[1];
        var p2 = params.split('/');

        if (p2.length) {
            p2.forEach(function(elem) {
                const regex = /^(\d+)-/gm;
                let res = regex.exec(elem)
                if (res) {
                    // console.log(res)
                    res = parseInt(res[1]);

                    if (res > 0) {
                        if ($('[value="' + res + '"]').length) {
                            if ($('[value="' + res + '"]').parent().is("select")) {
                                $('[value="' + res + '"]').parent().val(res);
                                $('[value="' + res + '"]').parent().trigger('change');
                            } else {
                                $('[value="' + res + '"]').trigger('click');
                            }
                        }
                    }
                }
            })

          $('[name="qty"]').trigger('change') //wymuszenie reload
          prestashop.emit("updateProduct", {})
        }
    }
}

jQuery(window).load(function () {
  setTimeout(function(){
    ReloadFeaturesOnProductBaseOnURL()
  }, 250)
});
window.addEventListener( "hashchange", function( event ) {
  ReloadFeaturesOnProductBaseOnURL()
});</script>
                <!-- End Custom JS -->
                    
