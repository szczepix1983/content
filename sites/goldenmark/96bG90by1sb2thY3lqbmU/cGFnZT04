	<script>
        $(document).ready(function () {
            $('.close-cart').click(function () {
                $('body').removeClass('body-cart-open');
                $('.cart-open').remove();
                if (prestashop.language.is_rtl == 1) {
                    $('#popup-cart').animate({ 'left': '-100%'}, 300);

                } else {
                    $('#popup-cart').animate({ 'right': '-100%'}, 300);
                }
            });
        });
    </script>
