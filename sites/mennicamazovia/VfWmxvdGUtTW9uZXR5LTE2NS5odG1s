   function wysylka(produkt) {
       var link = "link" + produkt;
       link = document.getElementById(link).href;

       if ((link.indexOf("24h") > -1)||(link.indexOf("24H") > -1)) {
            document.getElementById("dostawa"+produkt).style.display = "initial";
       }

       if ((link.indexOf("Przedsprzedaz") > -1)||(link.indexOf("przedsprzedaz") > -1)) {
            document.getElementById("przedsprzedaz"+produkt).style.display = "initial";
       }
   }

