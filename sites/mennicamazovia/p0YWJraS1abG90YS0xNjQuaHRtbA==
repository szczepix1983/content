const isOnCouponPage = window.location.href.includes('client-rebate.php');

if (basketHasProducts)
{
    if (hasCouponApplied) {
        basketContainer.append(activeCouponForm);
    } else {
        basketContainer.append(unactiveCouponForm);
    }
}

if (isOnCouponPage)
{
    document.body.style.display = 'none';
    window.location.replace("https://mennicamazovia.pl/basketedit.php");
}

</script>
