<script>

const elements = Array.from(document.getElementsByClassName('product-name'));
const images = Array.from(document.getElementsByClassName('product-icon'));

elements.forEach((item, index) => {
    let ribbonSet = false;

    if (item.text.includes('PRZEDSPRZEDAŻ')) {
        images[index].children[0].children[0].insertAdjacentHTML('beforeend', `
            <img src="https://mennicamazovia.pl/data/include/cms/img/ribbons/przedsprzedaz.gif" style="position: absolute; top: 0; left: 0">
        `);

        ribbonSet = true;
    }

    if (item.text.includes('PROMOCJA')) {
        images[index].children[0].children[0].insertAdjacentHTML('beforeend', `
            <img src="https://mennicamazovia.pl/data/include/cms/img/ribbons/promocja.gif" style="position: absolute; top: 0; left: 0">
        `);

        ribbonSet = true;
    }

    if (ribbonSet == false && item.text.includes('LIMITOWANA')) {
        images[index].children[0].children[0].insertAdjacentHTML('beforeend', `
            <img src="https://mennicamazovia.pl/data/include/cms/img/ribbons/limitowana.gif" style="position: absolute; top: 0; left: 0">
        `);

        ribbonSet = true;
    }

    if (ribbonSet == false && item.text.includes('NOWOŚĆ')) {
        images[index].children[0].children[0].insertAdjacentHTML('beforeend', `
            <img src="https://mennicamazovia.pl/data/include/cms/img/ribbons/nowosc.gif" style="position: absolute; top: 0; left: 0">
        `);

        ribbonSet = true;
    }

    if (ribbonSet == false && (item.text.includes('24H') || item.text.includes('24h'))) {
        images[index].children[0].children[0].insertAdjacentHTML('beforeend', `
            <img src="https://mennicamazovia.pl/data/include/cms/img/ribbons/wysylka.gif" style="position: absolute; top: 0; left: 0">
        `);

        ribbonSet = true;
    }
});

</script>

