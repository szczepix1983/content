	$('#menu1, #info1').hover(function() {
	    timer = setTimeout(function() {
	        $("#info1").addClass("visible");
	    }, delay);
	}, function() {
	    clearTimeout(timer);
	});

	$('#menu2, #info2').hover(function() {
	    timer = setTimeout(function() {
	        $("#info2").addClass("visible");
	    }, delay);
	}, function() {
	    clearTimeout(timer);
	});

	$("#menu1").mouseleave(function(){
  		$("#info1").removeClass("visible");
	});
	$("#menu2").mouseleave(function(){
  		$("#info2").removeClass("visible");
	});

</script><div class="footwrapper"><div class="foot"><div class="footnav"><div class="fn fn1"><span class="footnavtitle">Zamówienia</span><ul><li><a href="https://mennicamazovia.pl/client-orders.php?display=closed" rel="nofollow">Moje zamówienia</a></li><li><a href="https://mennicamazovia.pl/client-orders.php?display=opened" rel="nofollow">Sprawdź status zamówienia</a></li><li><a href="https://mennicamazovia.pl/rma-add.php" rel="nofollow">Złóż reklamację</a></li><li><a href="https://mennicamazovia.pl/products-requests.php">Szukam produktu</a></li></ul></div><div class="fn fn2"><span class="footnavtitle">Moje Konto</span><ul><li><a href="https://mennicamazovia.pl/loginedit.php?operation=edit" rel="nofollow">Twoje dane</a></li><li><a href="https://mennicamazovia.pl/basketedit.php" rel="nofollow">Twój koszyk</a></li><li><a href="https://mennicamazovia.pl/basketedit.php?mode=2" rel="nofollow">Lista ulubionych</a></li><li><a href="https://mennicamazovia.pl/client-orders.php?display=closed" rel="nofollow">Historia transakcji</a></li></ul></div><div class="fn fn3"><span class="footnavtitle">Regulaminy</span><ul><li><a href="https://mennicamazovia.pl/Regulamin-cterms-pol-18.html">Regulamin sklepu</a></li><li><a href="https://mennicamazovia.pl/Polityka-Prywatnosci-cinfo-pol-20.html">Polityka prywatności</a></li><li><a href="https://mennicamazovia.pl/Wysylka-cterms-pol-14.html">Opcje i koszty wysyłki</a></li><li><a href="https://mennicamazovia.pl/Sposoby-platnosci-i-prowizje-cterms-pol-17.html">Sposoby płatności i prowizje</a></li></ul></div><div class="fn fn4"><span class="footnavtitle">Informacje</span><ul><li><a href="https://mennicamazovia.pl/O-nas-cterms-pol-19.html">O Mennicy Mazovia</a></li><li><a href="https://mennicamazovia.pl/Czesto-Zadawane-Pytania-FAQ-cterms-pol-30.html">Częste pytania (FAQ)</a></li><li><a href="https://mennicamazovia.pl/blog-list.php">Blog</a></li><li><a href="https://mennicamazovia.pl/news-pol.phtml">Aktualności</a></li><li><a href="https://menni